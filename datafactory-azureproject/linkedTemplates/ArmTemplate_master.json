{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory-azureproject"
		},
		"AzureDataLakeStorageTarget_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageTarget'"
		},
		"AzureSqlDatabaseSource_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabaseSource'"
		},
		"AzureDataLakeStorageTarget_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageforazureproject.dfs.core.windows.net/"
		},
		"AzureSqlDatabaseSource_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "azureserverajay.database.windows.net"
		},
		"AzureSqlDatabaseSource_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "databaseajay"
		},
		"AzureSqlDatabaseSource_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminajay"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory-azureproject_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageTarget_accountKey": {
						"value": "[parameters('AzureDataLakeStorageTarget_accountKey')]"
					},
					"AzureSqlDatabaseSource_password": {
						"value": "[parameters('AzureSqlDatabaseSource_password')]"
					},
					"AzureDataLakeStorageTarget_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageTarget_properties_typeProperties_url')]"
					},
					"AzureSqlDatabaseSource_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabaseSource_properties_typeProperties_server')]"
					},
					"AzureSqlDatabaseSource_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabaseSource_properties_typeProperties_database')]"
					},
					"AzureSqlDatabaseSource_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabaseSource_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}